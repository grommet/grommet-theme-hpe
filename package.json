{
  "name": "grommet-theme-hpe",
  "version": "5.1.0",
  "main": "dist/index.js",
  "module": "dist/es6/index.js",
  "jsnext:main": "dist/es6/index.js",
  "sideEffects": false,
  "description": "Hewlett Packard Enterprise theme for grommet",
  "authors": [
    "Alan Souza",
    "Bryan Jacquot",
    "Chris Carlozzi",
    "Eric Soderberg",
    "Shimrit (Shimi) Yacobi"
  ],
  "bugs": "https://github.com/grommet/grommet-theme-hpe/issues",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/grommet/grommet-theme-hpe.git"
  },
  "peerDependencies": {
    "grommet": "^2.32.2",
    "grommet-icons": "^4.10.0",
    "react": "^16.13.1 || ^17.0.1 || ^18.0.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "del": "^6.1.1",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "fs-extra": "^11.1.0",
    "grommet-icons": "^4.10.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.1",
    "react": "^18.0.0",
    "simple-git": "^3.16.0",
    "styled-components": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "prepublishOnly": "npm run build && npm run jsonify",
    "build": "webpack --mode production && cross-env babel ./src/js/ --out-dir ./dist --copy-files && cross-env BABEL_ENV=es6 babel ./src/js/ --out-dir ./dist/es6 --copy-files",
    "lint": "eslint src",
    "lint-fix": "eslint src --fix",
    "prettier": "pretty-quick --staged",
    "release-stable": "babel-node ./tools/release-stable",
    "release-next-stable": "babel-node ./tools/release-next-stable",
    "release-audit-stable": "babel-node ./tools/release-audit-stable",
    "release-tokens-stable": "babel-node ./tools/release-tokens-stable",
    "jsonify": "babel-node ./tools/convert-to-json > dist/grommet-theme-hpe.json",
    "release-gh-pages": "babel-node ./tools/release-gh-pages"
  },
  "pre-commit": [
    "lint-fix",
    "prettier"
  ]
}
